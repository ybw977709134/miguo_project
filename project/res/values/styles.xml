<resources>



    <style name="page_title">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="contact_subtitle">
        <!-- Contact Page / list view group title -->
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:background">@drawable/category_bar</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/contact_subtitle</item>
    </style>

    <style name="contact_list_main_text">
        <!-- Contact Page / list view -->
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/default_text</item>
    </style>

	<style name="search_inputbox">
		<item name="android:layout_height">@dimen/search_bar_inner_height</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:paddingLeft">10dp</item>
        <item name="android:background">@drawable/table_white</item>
		<item name="android:drawableStart">@drawable/search_field_icon</item>
		<item name="android:drawablePadding">@dimen/padding_small</item>
		<item name="android:singleLine">true</item>
		<item name="android:textSize">@dimen/text_size_default</item>
		<item name="android:hint">@string/search</item>
	</style>

	<style name="side_index_bar">
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_width">30dp</item>
		<item name="android:layout_alignParentRight">true</item>
		<item name="android:textSize">15sp</item>
	</style>

    <style name="contact_list_sub_text">
        <!-- Contact Page / list view -->
        <item name="android:textSize">16sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/default_subtext</item>
    </style>

    <style name="group_member_counts_text">
        <!-- Contact Page / list view -->
        <item name="android:textSize">18sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/group_member_counts_gray</item>
    </style>

